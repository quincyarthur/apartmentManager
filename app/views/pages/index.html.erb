<h1>Welcome to Apartment Manager</h1>

<%if tenant_signed_in?%>
<%= link_to destroy_tenant_session_path, class: 'btn btn-default', method: :delete do%>
		Tenant Log Out
		<span class="glyphicon glyphicon-user"></span>
	<%end%>
<%else%>
	<%= link_to new_tenant_session_path, class: 'btn btn-default', method: :get  do%>
		Tenant Log In
		<span class="glyphicon glyphicon-user"></span>
	<%end%>
<%end%>

<%if landlord_signed_in?%>
	<%= link_to destroy_landlord_session_path, class: 'btn btn-default', method: :delete do%>
		Landlord Log Out
		<span class="glyphicon glyphicon-user"></span>
	<%end%>
<%else%>
	<%= link_to new_landlord_session_path, class: 'btn btn-default', method: :get do%>
		Landlord Log In
		<span class="glyphicon glyphicon-user"></span>
	<%end%>
<%end%>

<%@categories.each do |category|%>
	<div>
		<%=link_to category.name, properties_path(id: category.id)%>
	</div>	
<%end%>

<% console %>
